{
    "name": "accordous/inter-client",
    "description": "Client for Inter API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Accordous",
            "email": "contato@accordous.com.br"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Accordous\\InterClient\\Providers\\InterClientServiceProvider"
            ],
            "aliases": {
              "InterClient" : "Accordous\\InterClient\\Facades\\InterClient"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Accordous\\InterClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accordous\\InterClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
